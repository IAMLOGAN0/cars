# Server Configration
PROJECT_NAME="cars"
COMPOSE_PROJECT_NAME="cars"
APP_PROTOCOL="https"
APP_URL="https://cars.onrender.com"   # Replace with your actual Render URL
APP_PORT="10000"                      # Or the port your app listens on
ENCRYPTION_KEY="yoursecretkey"        # Any secure random string
NODE_ENVIRONMENT="production"
IS_LABDA_ENVIRONMENT="false"
ADMIN_URL="https://cars.onrender.com/admin"  # If you have admin panel

# DATABASE CONFIGRATION
DB_HOST="yourdbhost.render.com"       # From Render database settings
DB_NAME="yourdbname"                  # From Render database settings
DB_USER="yourdbuser"                  # From Render database settings
DB_PASSWORD="yourdbpassword"          # From Render database settings

# AWS Configration
AWS_ID="yourawsaccesskey"
AWS_SECRET="yourawssecretkey"
AWS_BUCKET="yourbucketname"
AWS_REGION="yourregion"
AWS_URL="https://yourbucketname.s3.amazonaws.com"

# Cognito Configration
COGNITO_ADMIN_POOL_ID=""
COGNITO_ADMIN_CLIENT_ID=""
COGNITO_ADMIN_CLIENT_SECRET=""
COGNITO_EDUCATOR_POOL_ID=""
COGNITO_EDUCATOR_CLIENT_ID=""
COGNITO_EDUCATOR_CLIENT_SECRET=""
COGNITO_VOLUNTEER_POOL_ID=""
COGNITO_VOLUNTEER_CLIENT_ID=""
COGNITO_VOLUNTEER_CLIENT_SECRET=""

# SMTP Configration
SMTP_HOST="smtp.yourmailprovider.com"
SMTP_PORT="587"
SMTP_USERNAME="your@email.com"
SMTP_PASSWORD="youremailpassword"
SMTP_MAIL_ADDRESS="your@email.com"
SMTP_SECURE="false"

# Extra Settings
IS_LIVE_OTP="false"
ALTER_TABLE="false"
SQL_LOG="false"
APP_TIMEZONE="Asia/Kolkata"
